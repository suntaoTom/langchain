# LangGraph / Model Settings
DEFAULT_MODEL=gemini
GOOGLE_API_KEY=
DASHSCOPE_API_KEY=
DEEPSEEK_API_KEY=

# Ollama Settings
OLLAMA_MODEL=deepseek-v3.1:671b-cloud
OLLAMA_BASE_URL=http://127.0.0.1:11434

# MCP Settings (Optional)
# List of servers separated by comma
MCP_SERVERS=figma
# Command to run each server
MCP_SERVER_FIGMA_CMD=npx -y @modelcontextprotocol/server-figma
# Required API keys for specific MCP servers
FIGMA_ACCESS_TOKEN=your_figma_token_here

# LangSmith Settings (Monitoring)
LANGSMITH_TRACING=false
LANGSMITH_PROJECT="langchain-agent"
LANGSMITH_API_KEY=
LANGCHAIN_TRACING_V2=true

# Project Context
PROJECT_ROOT=.
PROJECT_EXCLUDES=.git,__pycache__,.venv,node_modules,.agent,agent.egg-info,static,.langgraph_api
